{
  "manifest_version": 3,
  "name": "AEM Quick Check",
  "version": "1.0.2",
  "description": " AEM Quick Check Instantly access AEM Page data, debug tools, and SEO checks for faster workflows and team collaboration.",
  "permissions": ["storage", "scripting", "activeTab"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
   },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "AEM Quick Check"
  },
  "host_permissions": ["<all_urls>", "*://*/*"]
}